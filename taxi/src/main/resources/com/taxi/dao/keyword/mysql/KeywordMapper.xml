<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.taxi.dao.keyword.KeywordDao">

	<select id="getKeywordList" resultType="MyInfo"> 
		select 
				KEYWORD_NO 		as keywordNo
			,	KEYWORD_NAME 	as keywordName			
			,	KEYWORD_ST 		as keywordSt
			
		from 	
				KEYWORD
		where 	
				1 = 1
		
    </select>
 
</mapper> 













