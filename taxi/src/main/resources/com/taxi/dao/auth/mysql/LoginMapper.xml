<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.taxi.dao.auth.LoginDao">
    
	<delete id="deleteLoginData" parameterType="int">
	    delete from LOGIN  
        where MBR_NO=#{value}
	</delete>
 
</mapper> 













